    {
        "id": "fetch_refugee_for_recommendation",
        "type": "postgresql",
        "z": "tab_ai_integration",
        "name": "Obtener refugiado",
        "query": "SELECT * FROM refugees WHERE id = $1",
        "postgreSQLConfig": "301245f94e1239d9",
        "split": false,
        "rowsPerMsg": "",
